<!DOCTYPE html>
<html>
<head>
  <title>WellnessTracker</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= stylesheet_link_tag    'materialize', media: 'all'%>
  <%= stylesheet_link_tag    'materialize.min', media: 'all'%>
  <%= javascript_include_tag 'application' %>
  <%= javascript_include_tag 'materialize' %>
  <%= javascript_include_tag 'materialize.min' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="nav">
    <% if current_user %>
      <%= link_to "Edit Profile", edit_profile_path %>
      <%= link_to "Logout", :logout, method: :delete %>
    <% else %>
      <%= link_to "Register", new_user_path %> |
      <%= link_to "Login", :login %>
    <% end %>
  </div>
  <div>
    <p id="notice"><%= flash[:notice] %></p>
    <p id="alert"><%= flash[:alert] %></p>
  </div>
  <%= yield %>

</body>
</html>
